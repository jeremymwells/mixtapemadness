{
  "name": "mixtapemadness",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "lint:ci": "npx nx run-many --target=lint --projects=serverless-angular,angular --ci",
    "unit-test-client": "npx nx run angular:test --ci --coverage",
    "integration-test-client": "echo 'make this run e2e' && echo 0",
    "unit-test-api": "npx nx run serverless-angular:test --ci --coverage",
    "integration-test-api": "echo 'make this run api cypress shit' && echo 0",
    "migrations:up": "cd ./libs/serverless/angular && npm run migrations:up && cd -",
    "migrations:down": "cd ./libs/serverless/angular && npm run migrations:down && cd -",
    "migrations:create": "cd ./libs/serverless/angular && npm run migrations:create && cd -",
    "build": "npx nx run-many --target=build --projects=serverless-angular,angular",
    "start": "npx nx run-many --target=serve --projects=serverless-angular,angular",
    "api": "cd libs/serverless/angular && npm start && cd -",
    "client": "npx nx run angular:serve",
    "asd": "concurrently --kill-others \"npm run local:api\" \"npm run local:client\"",
    "predeploy:prod": "npm run build",
    "predeploy:dev": "npm run build",
    "predeploy:nonprod": "npm run build --configuration=\"development\"",
    "deploy:nonprod": "cd libs/serverless/angular && npm run deploy:nonprod && cd -",
    "deploy:prod": "cd libs/serverless/angular && npm run deploy:prod && cd -",
    "deploy:dev": "cd libs/serverless/angular && npm run deploy:dev && cd -",
    "preinstall": "cd libs/serverless/angular && npm i && cd -",
    "preci": "cd libs/serverless/angular && npm ci && cd -"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
