    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "15.2.0",
    "@angular/compiler-cli": "15.2.0",
    "@angular/language-service": "15.2.0",
    "@aws-sdk/types": "^3.485.0",
    "@nrwl/angular": "16.0.0",
    "@nrwl/cypress": "16.0.0",
    "@nrwl/eslint-plugin-nx": "16.0.0",
    "@nrwl/jest": "16.0.0",
    "@nrwl/js": "16.0.0",
    "@nrwl/linter": "16.0.0",
    "@nrwl/web": "16.0.0",
    "@nrwl/workspace": "16.0.0",
    "@nx/angular": "16.0.0",
    "@nx/cypress": "16.0.0",
    "@nx/jest": "16.0.0",
    "@nx/js": "16.0.0",
    "@schematics/angular": "15.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
