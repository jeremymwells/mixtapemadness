{
  "name": "mixtapemadness",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint:ci": "npx nx run-many --target=lint --projects=serverless-angular,angular --ci",
    "unit-test-client": "npx nx run angular:test --ci --coverage",
    "integration-test-client": "echo 'make this run e2e' && echo 0",
    "unit-test-api": "npx nx run serverless-angular:test --ci --coverage",
    "integration-test-api": "echo 'make this run api cypress shit' && echo 0",
    "migrations": "npx nx run serverless-angular:migrations",
    "domainName": "npx nx run serverless-angular:domainName",
    "build": "npx nx run-many --target=build --projects=serverless-angular,angular",
    "start": "npx nx run-many --target=serve --projects=serverless-angular,angular",
    "api": "cd libs/serverless/angular && npm start && cd -",
    "asd": "concurrently --kill-others \"npm run local:api\" \"npm run local:client\"",
    "predeploy:prod": "npm run build",
    "deploy:nonprod": "npx nx run serverless-angular:deploy",
    "deploy:prod": "npx nx run serverless-angular:deploy --stage=prod",
    "deploy:dev": "npx nx run serverless-angular:deploy --stage=dev",
    "preinstall": "cd libs/serverless/angular && npm i --legacy-peer-deps=true && cd -",
    "preci": "cd libs/serverless/angular && npm ci --legacy-peer-deps=true && cd -"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.0",
    "@angular-devkit/core": "15.2.0",
    "@angular-devkit/schematics": "15.2.0",
