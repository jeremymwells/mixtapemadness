    "@angular/cli": "15.2.0",
    "@angular/compiler-cli": "15.2.0",
    "@angular/language-service": "15.2.0",
    "@nrwl/angular": "16.0.0",
    "@nrwl/cypress": "16.0.0",
    "@nrwl/eslint-plugin-nx": "16.0.0",
    "@nrwl/jest": "16.0.0",
    "@nrwl/js": "16.0.0",
    "@nrwl/linter": "16.0.0",
    "@nrwl/web": "16.0.0",
    "@nrwl/workspace": "16.0.0",
    "@nx/angular": "16.0.0",
    "@schematics/angular": "15.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "concurrently": "^8.0.1",
    "cypress": "^12.2.0",
    "eslint": "8.15.0",
